    },
    {
      "cell_type": "code",
      "source": [
        "#model init\n",
        "adaBur = AdalineGD(eta=0.01, n_iter=50)\n",
        "adaBur2 = AdalineGD(eta=0.0001, n_iter=50)\n",
        "adaBur3 = AdalineGD(eta=0.00001, n_iter=50)\n",
        "#model training\n",
        "adaBur.fit(Bur_X_train, Bur_y_train, Bur_X_val, Bur_y_val)\n",
        "adaBur2.fit(Bur_X_train, Bur_y_train, Bur_X_val, Bur_y_val)\n",
        "adaBur3.fit(Bur_X_train, Bur_y_train, Bur_X_val, Bur_y_val)\n",
        "\n",
        "#plot the models\n",
